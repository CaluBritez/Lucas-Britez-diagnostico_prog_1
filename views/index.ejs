<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% posts.forEach(post => { %>
      <li>
        <h1><%= post.title %></h1>
        <h3><%= post.description %></h3>
        <h4>Author: <%= post.author %></h4>
      </li>
      <% }) %>
    </ul>
    <!-- Mostrar los posts aquÃ­ -->
  </body>
</html>
